
.right_aligned {
	height: 10em;
	line-height: 1.25em;
}

.right_aligned label {
	display: block;
	float: left;
	clear: left;
	width: 30%;
	padding-right: 1em;
	text-align: right;
}

.right_aligned select, .right_aligned input {
	display: block;
}

.right_aligned textarea{
	width: 40%;
	height: 40%;
}


table{
	margin: 5px 5px 5px 5px;
	padding: 5px;
}

table.altrowstable {
	font-family: verdana,arial,sans-serif;
	font-size:11px;
	color:#333333;
	border-width: 1px;
	border-color: #a9c6c9;
	border-collapse: collapse;
}
table.altrowstable th {
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #a9c6c9;
	color: black;
	background-color: 8C8984;
}
table.altrowstable td {
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #a9c6c9;
}
.oddrowcolor{
	background-color:#EBE9E7;
}
.evenrowcolor{
	background-color:#D1ECF7;
}

