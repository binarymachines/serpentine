#header
{
    position: absolute; 
    top: 0;
    left: 0;
    height: 96px;
    width: 100%;
	font-family: Arial, Helvetica, sans-serif;
	background: #005B9A;
	/* padding: 20px; */
	color: #eeeeee;
}

#header h1 { 
	margin-left: 20px;
	margin-bottom: 0px;
	letter-spacing: -1;
}

#header p{
	margin-top: 0px;
	margin-bottom: 10px;
	margin-left: 20px;
	letter-spacing: 1;
}

#navigation
{
    position: absolute;
    display: block;
	background-color: #8C8984;
	float: left;
	left: 0;
	top: 96px;
	height: 30px;
	width: 100%;
	font-family: Tahoma, Geneva, sans-serif;
	color: gray;
}

#navigation ul
{
	margin: 0;
	padding: 0;
}

#navigation ul li
{
	list-style-type: none;
	display: inline;
}

#navigation li a
{
	display: block;
	float: left;
	padding: 5px 10px;
	color: #fff;
	text-decoration: none;
	border-right: 1px solid #fff;
}

#navigation li a:hover { background: #0191C8; }

#content-container
{
	/* float: left; */
	position: relative;
	top: 106px;
	width: 100%;
	height: 100%;
	/* min-height: 600px; */
	margin-top: 10px;
	margin-bottom: 0px;
	display: block;
	background:silver;
	/*
	border-width: 1px;
    border-color: red;
    border-style: solid;
    */
}

#content-header
{
	/* clear: left; */
	/* float: left; */
	position: relative;
	top: 0px;
	width: 100%;
	height: 100px;
	margin-left: auto;
	margin-right: auto;
	padding-top: 5px;
	margin-top: 20px;
	display: block;
	font-family: Tahoma, Geneva, sans-serif;
    background: white;
    
	border-width: 0px;
    border-color: white;
    border-style: solid; 
}

#content-header h2 {
	margin-left: 20px; 
}

#content{
    
    font-family: Tahoma, Geneva, sans-serif;
    position: relative;
    height: 100%;
    min-height: 700px; 
    width: 80%;
    margin-left: auto;
    margin-right: auto;
    padding-bottom: 20px;
    background: white;
    /*
    border-width: 1px;
    border-color: black;
    border-style: dashed;
    */
}

#content p {
	margin-left: 20px; 
}

#content form{
    margin-left: 20px;
}



#menu
{
	float: right;
	width: 26%;
	padding: 20px 0;
	margin: 0 3% 0 0;
	display: block;
	font-family: Arial, Helvetica, sans-serif;
}

#menu h3 { margin: 0; }

#footer
{
	font-family: Tahoma, Geneva, sans-serif;
	display: block;
	background: #ccc;
	padding: 10px;
	height: 10px;
	position: fixed;
	bottom: 0;
	text-align: center;
	vertical-align: middle;
	width: 100%;
}

#footer a{
	/* color: #fff; */
	text-decoration: none;
}

